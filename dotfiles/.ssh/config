Host remote.github.net remote.github.com
	ForwardAgent yes
	User christippett

Host *.github.com *.github.net *.githubapp.com
	ForwardAgent no
	User christippett

Host *
	StrictHostKeyChecking ask
	VerifyHostKeyDNS ask
	NoHostAuthenticationForLocalhost yes
	IdentityFile ~/.ssh/id_rsa
	AddKeysToAgent yes

Host windows
    Hostname desktop.local
    User ctipp

Host wsl
    HostName desktop.local
	User chris
	Port 2222

Host nas
    HostName tennis-bot-3000.local
    User chris

Host laptop
	HostName chriss-macbook-pro
	User chris


Host cloud-shell
	HostName devshell-vm-e826093c-51b2-48fb-8af7-b1788c3e3ed4.cloudshell.dev
	Port 6000
